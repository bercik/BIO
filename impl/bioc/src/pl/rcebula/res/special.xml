<functions>
    <!-- funkcje specjalne-->
    <!-- funkcje które są zamieniane na odpowiedni kod w trakcie kompilacji 
           (nie znajdziemy ich po stronie interpretera -->
    <function>
        <name>
            FOR
        </name>
        <special>
            true
        </special>
        <params>
            <param>
                call
            </param>
            <param>
                all
            </param>
            <param>
                call
            </param>
            <param>
                call
            </param>
        </params>
    </function>
    
    <function>
        <name>
            IF
        </name>
        <special>
            true
        </special>
        <params>
            <param>
                all
            </param>
            <param>
                call
            </param>
            <param optional="true">
                call
            </param>
        </params>
    </function>
    
    <function>
        <name>
            CALL
        </name>
        <special>
            true
        </special>
        <params>
            <param>
                call
            </param>
            <param repeat="true">
                call
            </param>
        </params>
    </function>
    
    <function>
        <name>
            DN
        </name>
        <special>
            true
        </special>
        <params>
        </params>
    </function>
    
    <function>
        <name>
            BREAK
        </name>
        <special>
            true
        </special>
        <params>
        </params>
    </function>
    
    <function>
        <name>
            CONTINUE
        </name>
        <special>
            true
        </special>
        <params>
        </params>
    </function>
</functions>